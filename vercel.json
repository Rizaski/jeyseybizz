{
    "version": 2,
    "builds": [{
        "src": "*.html",
        "use": "@vercel/static"
    }],
    "routes": [{
            "src": "/",
            "dest": "/index.html"
        },
        {
            "src": "/admin-panel",
            "dest": "/admin-panel.html"
        },
        {
            "src": "/login",
            "dest": "/login.html"
        },
        {
            "src": "/contact",
            "dest": "/contact.html"
        }
    ],
    "headers": [{
        "source": "/(.*)",
        "headers": [{
                "key": "X-Content-Type-Options",
                "value": "nosniff"
            },
            {
                "key": "X-Frame-Options",
                "value": "DENY"
            },
            {
                "key": "X-XSS-Protection",
                "value": "1; mode=block"
            }
        ]
    }]
}