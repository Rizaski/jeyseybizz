{
    "version": 2,
    "functions": {
        "api/index.js": {
            "runtime": "nodejs18.x"
        }
    },
    "routes": [
        {
            "src": "/",
            "dest": "/index-static.html"
        },
        {
            "src": "/about",
            "dest": "/about.html"
        },
        {
            "src": "/services",
            "dest": "/services.html"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}